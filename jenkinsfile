properties([pipelineTriggers([githubPush()])])
pipeline{
    agent any 
          
        stages {
            stage('Checkout GIT ') {
                steps {
                    echo 'Pulling ...';
                    git branch : 'master',
                    url : 'https://github.com/Majdi-msallem/Spring-test.git/';
                }
            }
     
             stage('MVN Version') {
            steps {
                sh 'mvn -version'
            }
        }
      
         
        
        }
}
